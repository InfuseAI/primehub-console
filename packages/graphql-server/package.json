{
  "name": "@infuseai/graphql-server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=development ts-node src/ee/bin/www.ts",
    "start:lib": "NODE_ENV=development node lib/ee/bin/www",
    "start:prod": "NODE_ENV=production node lib/ee/bin/www",
    "start:ce": "NODE_ENV=development ts-node src/bin/www.ts",
    "start:ce:lib": "NODE_ENV=development node lib/bin/www.ce",
    "start:ce:prod": "NODE_ENV=production node lib/bin/www.ce",
    "clean": "rimraf lib",
    "lint": "tslint --force --format verbose \"src/**/*.ts\" \"test/*\"",
    "build": "npm run clean && npm run lint && echo Using TypeScript && tsc --version && tsc -p ./tsconfig.release.json --pretty",
    "build:dryrun": "tsc -p ./tsconfig.release.json --noEmit --pretty",
    "build:prod": "npm run clean && echo Using TypeScript && tsc --version && tsc -p ./tsconfig.release.json --pretty && npm run copy:views && npm run copy:graphql",
    "build:ce": "npm run clean && npm run lint && echo Using TypeScript && tsc --version && tsc -p ./tsconfig.ce.json --pretty",
    "build:ce:dryrun": "tsc --version && tsc -p ./tsconfig.ce.json --noEmit --pretty",
    "build:ce:prod": "npm run clean && echo Using TypeScript && tsc --version && tsc -p ./tsconfig.ce.json --pretty && npm run copy:views && npm run copy:graphql",
    "copy:views": "cp -R src/views lib/views",
    "copy:graphql": "cp -R src/graphql lib/graphql; cp -R src/ee/graphql lib/ee/graphql",
    "test": "TEST=true mocha --compilers ts:ts-node/register --exit --timeout 600000 --file \"test/setup.ts\"  --recursive \"test/**/*.spec.ts\"",
    "test:grep": "TEST=true mocha --compilers ts:ts-node/register --exit --timeout 600000 --file \"test/setup.ts\" --recursive",
    "test:e2e": "TEST=true cucumber-js ./test/features/*.feature  --require ./test/features/support/**/*.ts --require-module ts-node/register",
    "test:build": "TEST=true tslint --format verbose \"src/**/*.ts\" && npm run build:dryrun && npm run build:dryrun",
    "coverage": "nyc npm run test",
    "watch": "npm run build -- --watch",
    "watch:test": "npm run test -- --watch"
  },
